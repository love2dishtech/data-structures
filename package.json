{
  "name": "data-structures",
  "version": "0.1.0",
  "description": "The data structures implementation inspired and inherited by the William Fiset",
  "repository": {
    "type": "git",
    "url": "https://github.com/love2dishtech/data-structures"
  },
  "author": "Love Hasija",
  "license": "MIT",
  "scripts": {
    "build": "npm run build-sass && npm run build-ts && npm run lint && npm run copy-static-assets",
    "build-ts": "tsc",
    "debug": "npm run build && npm run watch-debug",
    "lint": "tsc --noEmit && eslint \"**/*.{js,ts}\" --quiet --fix",
    "serve-debug": "nodemon --inspect dist/server.js",
    "start": "npm run serve",
    "test": "jest",
    "watch-test": "npm run test -- --watchAll",
    "watch-ts": "tsc -w"
  },
  "dependencies": {
    "lodash": "^4.17.15",
    "winston": "^3.2.1"
  },
  "devDependencies": {
    "@babel/preset-typescript": "^7.10.4",
    "@types/eslint": "^6.1.1",
    "@types/jest": "^26.0.3",
    "@types/lodash": "^4.14.141",
    "@types/node": "^14.0.14",
    "@types/winston": "^2.4.4",
    "@typescript-eslint/eslint-plugin": "^2.3.1",
    "@typescript-eslint/parser": "^2.3.1",
    "eslint": "^6.4.0",
    "jest": "^26.1.0",
    "ts-jest": "^26.1.1",
    "ts-node": "^8.4.1",
    "typescript": "^3.9.6"
  }
}
